-- database: ./w3schools.sqlite
SELECT
  *
FROM
  categories;

select distinct
  city
from
  customers
order by
  city;

select
  ContactName,
  PostalCode,
  City
from
  customers
where
  country = 'USA';

select
  *
from
  customers
where
  CustomerID != 81;

select
  *
from
  products
order by
  price asc;

select
  country,
  city
from
  customers
ORDER BY
  country;
